		"health": "curl http://localhost:3000/api/health || echo 'Health check failed'"
	},
	"dependencies": {
		"@heroicons/react": "^2.2.0",
		"framer-motion": "^12.23.12",
		"html2canvas": "^1.4.1",
		"lucide-react": "^0.543.0",
		"next": "15.5.2",
		"next-intl": "^4.3.7",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-dropzone": "^14.3.8",
		"react-hot-toast": "^2.6.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.5.2",
		"tailwindcss": "^4",
		"typescript": "^5"
